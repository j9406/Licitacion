@model IEnumerable<PCILicitacion.Model.LicitacionPartida>

@{
    ViewBag.Title = "Partida";
}

<h2>Partida</h2>

<p>
    @Html.ActionLink("Nueva", "LicitacionCreate", new { Id = ViewBag.Id })
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Observaciones)
        </th>
        <th></th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Observaciones)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
        <td>
            @Html.ActionLink("Productos", "LicitacionPartida", "LicitacionPartidaProducto", new { Id = item.Id }, null)
        </td>
    </tr>
}

</table>
